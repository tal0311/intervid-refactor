{
  "name": "intervid-front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --no-fix --max-warnings 0 --ignore-path .gitignore",
    "format": "prettier --write src/ --ignore-path .gitignore",
    "format:check": "prettier --check src/ --ignore-path .gitignore",
    "lint-format": "npm run lint && npm run format",
    "check-lint-format": "npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@uppy/aws-s3": "^3.0.6",
    "@uppy/aws-s3-multipart": "^3.1.3",
    "@uppy/core": "^3.1.2",
    "@uppy/drag-drop": "^3.0.1",
    "axios": "^1.3.2",
    "axios-retry": "^3.4.0",
    "caniuse-lite": "^1.0.30001450",
    "detect-browser": "^5.3.0",
    "face-api.js": "^0.20.0",
    "file-saver": "^2.0.5",
    "material-icons": "^1.13.1",
    "quill": "^1.3.7",
    "register-service-worker": "^1.7.2",
    "sass": "^1.58.0",
    "socket.io-client": "^4.6.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-hotjar-next": "^1.4.0",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.1.6",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vue3-google-login": "^2.0.15",
    "vue3-observe-visibility": "^0.1.2",
    "vue3-touch-events": "^4.1.0",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.2.6",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "lodash.clonedeep": "^4.5.0",
    "prettier": "^2.7.1",
    "vite": "^4.0.0",
    "vitest": "^0.30.1"
  }
}
